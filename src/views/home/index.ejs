    // Function to generate a random user name
    function generateRandomUserName() {
      var names = ['mem', 'mem', 'mem', 'mem', 'mem', 'mem', 'mem', 'mem', 'mem', 'mem'];
      var randomIndex = Math.floor(Math.random() * names.length);
      var randomName = names[randomIndex];
      var randomInitials = Math.floor(Math.random() * 10000); // Adding random numbers for uniqueness
      return randomName + '***' + randomInitials;
    }

    // Function to generate a random image URL
    function generateRandomImageURL() {
      var images = [
        '/assets/png/8-ea087ede.png',
        '/assets/png/avatar-fa220f51.png',
        'https://www.bigdaddygame2.com/assets/png/1-c7e4efc8.png',
